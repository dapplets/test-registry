{
  "name": "dapplet-registry",
  "version": "0.1.0",
  "description": "Test Dapplet Registry",
  "main": "src/app.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "build": "tsc",
    "test": "mocha -r ts-node/register ./test/**/*.spec.ts",
    "test:watch": "mocha -r ts-node/register --watch ./test/**/*.spec.ts --watch-extensions ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dapplets/dapplet-registry.git"
  },
  "keywords": [
    "dapplets",
    "registry"
  ],
  "author": "Dapplets Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dapplets/dapplet-registry/issues"
  },
  "homepage": "https://github.com/dapplets/dapplet-registry#readme",
  "dependencies": {
    "bs58": "^4.0.1",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/bs58": "^4.0.0",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/multer": "^1.3.7",
    "@types/rimraf": "^2.0.2",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
